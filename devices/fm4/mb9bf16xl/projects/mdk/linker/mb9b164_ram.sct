#define IROM1_START 0x2003E000
#define IROM1_SIZE  0x00002000
#define IRAM1_START 0x20040000
#define IRAM1_SIZE  0x00002000
#define IRAM2_START 0x1FFFC000
#define IRAM2_SIZE  0x00004000

LR_IROM1 IROM1_START IROM1_SIZE  {    ; load region size_region
  ER_IROM1 IROM1_START IROM1_SIZE  {  ; load address = execution address
   *.o (RESET, +First)
   *(InRoot$$Sections)
   .ANY (+RO)
  }
  RW_IRAM1 IRAM1_START IRAM1_SIZE  {  ; RW data
   .ANY (+RW +ZI)
  }
}
